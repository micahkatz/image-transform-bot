{
  "name": "micah-discord-bot",
  "version": "1.0.0",
  "description": "A discord bot that performs image transformations and allows for smart home device control",
  "main": "app.js",
  "type": "module",
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "build": "tsc",
    "start:ts": "tsc --outDir dist/ && node dist/app.js",
    "start": "node app.js",
    "start:audio": "nodemon audio.js",
    "register": "node commands.js",
    "dev": "nodemon app.js",
    "ngrok": "ngrok http --domain=civil-dear-ram.ngrok-free.app 3000"
  },
  "author": "Micah Katz",
  "license": "MIT",
  "dependencies": {
    "@discordjs/voice": "^0.17.0",
    "cloudinary": "^1.41.3",
    "discord-interactions": "^3.2.0",
    "discord.js": "^14.16.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.45",
    "ngrok": "^5.0.0-beta.2",
    "node-fetch": "^3.3.2",
    "openai": "^4.44.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.15",
    "typescript": "^5.6.3"
  }
}